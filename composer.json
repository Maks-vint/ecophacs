{
    "name": "welterrocks/ecophacs",
    "description": "PHP library for ecovacs API.",
    "keywords": [
        "php",
        "ecovacs",
        "deebot",
        "library"
    ],
    "license": "GPL-3.0-only",
    "authors": [
        {
            "name": "Oliver Welter",
            "email": "oliver@welter.rocks"
        }
    ],
    "type": "project",
    "require": {
        "php": ">=7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "6.*",
        "phpmd/phpmd": "2.6.0",
        "squizlabs/php_codesniffer": "3.4.2",
        "friendsofphp/php-cs-fixer": "3.0.x-dev",
        "norgul/xmpp-php": "2.2.3",
        "phpseclib/phpseclib": "2.*"
    },
    "autoload": {
        "psr-4": {
            "WelterRocks\\": "src",
            "Norgul\\Xmpp\\": "override/welterrocks/xmpp-php/src"
        }
    }
}
